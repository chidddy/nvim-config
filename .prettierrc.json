{
    "semi": true,
    "tabWidth": 4,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "avoid",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "editorconfig": true,
    "embeddedLanguageFormatting": "auto",
    "endOfLine": "lf",
    "useTabs": false,
    "singleAttributePerLine": false,
    "htmlWhitespaceSensitivity": "css",
    "jsxSingleQuote": true,
    "quoteProps": "as-needed",
    "printWidth": 100
}
